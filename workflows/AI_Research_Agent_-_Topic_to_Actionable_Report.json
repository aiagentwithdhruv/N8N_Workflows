lp gather comprehensive information. You should identify the main research focus and key aspects to investigate."}},"id":"e1c94261-f029-4edd-913d-df432e1b465b","name":"Research Planner","type":"@n8n/n8n-nodes-langchain.agent","typeVersion":3.1,"position":[-3328,176]},{"parameters":{"jsCode":"const plannerOutput = items[0].json.output;\nconst queries = plannerOutput.search_queries || [];\n\nreturn queries.map(query => ({\n  json: {\n    query: query,\n    topic: items[0].json.topic,\n    research_focus: plannerOutput.research_focus,\n    max_sources: items[0].json.max_sources\n  }\n}));"},"id":"31152a74-c566-4d9a-ac65-e6e7672c0ce4","name":"Extract Search Queries","type":"n8n-nodes-base.code","typeVersion":2,"position":[-2976,176]},{"parameters":{"fieldToSplitOut":"query","include":"selectedOtherFields","fieldsToInclude":"topic, research_focus, max_sources","options":{}},"id":"ef4af6b6-3728-4c69-b7a5-4d8d4492d67c","name":"Split Search Queries","type":"n8n-nodes-base.splitOut","typeVersion":1,"position":[-2752,176]},{"parameters":{"method":"POST","url":"https://api.tavily.com/search","sendHeaders":true,"headerParameters":{"parameters":[{"name":"Content-Type","value":"application/json"}]},"sendBody":true,"specifyBody":"json","jsonBody":"={\n  \"api_key\": \"YOUR_TAVILY_API_KEY\",\n  \"query\": \"={{ $json.query }}\",\n  \"max_results\": 5\n}","options":{}},"id":"5553c3a9-b889-4102-94d2-c9b9cb1069fe","name":"Tavily Web Search","type":"n8n-nodes-base.httpRequest","typeVersion":4.3,"position":[-2528,48]},{"parameters":{"url":"https://newsapi.org/v2/everything","sendQuery":true,"queryParameters":{"parameters":[{"name":"q","value":"={{ $json.query }}"},{"name":"apiKey","value":"YOUR_NEWSAPI_KEY"},{"name":"pageSize","value":"5"},{"name":"sortBy","value":"relevancy"}]},"options":{}},"id":"070c1f2f-b924-45a4-8326-bec5f3ab0a2d","name":"NewsAPI Search","type":"n8n-nodes-base.httpRequest","typeVersion":4.3,"position":[-2528,240]},{"parameters":{"url":"https://api.semanticscholar.org/graph/v1/paper/search","sendQuery":true,"queryParameters":{"parameters":[{"name":"query","value":"={{ $json.query }}"},{"name":"limit","value":"5"},{"name":"fields","value":"title,url,abstract,year"}]},"options":{}},"id":"a7793543-b737-4db7-8741-da873651ca90","name":"Semantic Scholar Search","type":"n8n-nodes-base.httpRequest","typeVersion":4.3,"position":[-2528,432]},{"parameters":{"numberInputs":3},"id":"61e32f23-e495-4265-b102-719395fa18b2","name":"Merge Search Results","type":"n8n-nodes-base.merge","typeVersion":3.2,"position":[-2304,160]},{"parameters":{"jsCode":"const allResults = [];\nconst maxSources = items[0].json.max_sources || 10;\n\nfor (const item of items) {\n  const data = item.json;\n  \n  // Tavily results\n  if (data.results) {\n    data.results.forEach(r => allResults.push({ url: r.url, title: r.title, score: r.score || 0.5 }));\n  }\n  \n  // NewsAPI results\n  if (data.articles) {\n    data.articles.forEach(a => allResults.push({ url: a.url, title: a.title, score: 0.6 }));\n  }\n  \n  // Semantic Scholar results\n  if (data.data) {\n    data.data.forEach(p => allResults.push({ url: p.url, title: p.title, score: 0.7 }));\n  }\n}\n\n// Remove duplicates and sort by score\nconst uniqueUrls = new Map();\nallResults.forEach(r => {\n  if (!uniqueUrls.has(r.url) || uniqueUrls.get(r.url).score < r.score) {\n    uniqueUrls.set(r.url, r);\n  }\n});\n\nconst topUrls = Array.from(uniqueUrls.values())\n  .sort((a, b) => b.score - a.score)\n  .slice(0, maxSources)\n  .map(r => r.url);\n\nreturn [{ json: { urls: topUrls, topic: items[0].json.topic } }];"},"id":"fcb36a32-f4ce-4858-869b-6d602be6817f","name":"Extract Top URLs","type":"n8n-nodes-base.code","typeVersion":2,"position":[-2080,176]},{"parameters":{"fieldToSplitOut":"urls","include":"selectedOtherFields","fieldsToInclude":"topic","options":{}},"id":"947c0c7b-dff4-4aab-8216-61fb0d63b43b","name":"Split URLs","type":"n8n-nodes-base.splitOut","typeVersion":1,"position":[-1856,176]},{"parameters":{"url"